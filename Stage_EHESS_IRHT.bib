@book{edmondDigitalTechnologyPractices2020,
  title = {Digital {{Technology}} and the {{Practices}} of {{Humanities Research}}},
  editor = {Edmond, Jennifer},
  year = {2020},
  journal = {Digital Technology and the Practices of Humanities Research},
  series = {Digital {{Humanities Series}}},
  publisher = {Open Book Publishers},
